;-------------------
;●マッサージ店
;-------------------
@REGISTER_TERRITORY_EVENT138(対象)
#DIM 対象
RETURN 1

@TERRITORY_EVENT138(対象)
#DIM 対象
#DIM 仲間
#DIM 候補キャラ, 1000
#DIM 候補数
VARSET 候補キャラ
VARSET 候補数
FOR LOCAL, 0, CHARANUM
	IF CFLAG:(LOCAL):所属 == CFLAG:MASTER:所属 && !CFLAG:(LOCAL):捕虜先 && IS_FEMALE(LOCAL) && !IS_ANIMAL(LOCAL)
		候補キャラ:LOCAL = RAND:1000 
		候補数 ++
	ENDIF
NEXT
IF 候補数 == 0
	仲間 = 0
ELSE
	仲間 = FINDELEMENT(候補キャラ, MAXARRAY(候補キャラ))
ENDIF
PRINTFORMW 探索中、マッサージ店を見つけた
IF MONEY < 3000
	PRINTFORMW しかしお金がないのでやめておいた
	RETURN 1
ENDIF
PRINTFORMW 最近戦続きで疲れていた%ANAME(対象)%は、試しに入ってみることにした
PRINTFORML 店内には落ち着いた音楽がかかっており、明るすぎない照明と合わせて心地よい雰囲気だ
PRINTFORML %ANAME(対象)%が入店するとすぐにマッサージ師がやってきて一室に案内された
PRINTFORML 中央に寝台のある一面ピンクの部屋だ、甘い香りのアロマも焚かれている
PRINTFORMW 彼に促されて上着を脱いで寝台に寝転ぶと、マッサージが始まった
PRINTFORML ・
PRINTFORML ・
PRINTFORMW ・
IF IS_MALE(対象)
	PRINTFORMW とても心地よかった 
	IF 仲間 != 0 && 対象 == MASTER
		PRINTFORML 優待チケットを貰ったので、%ANAME(仲間)%に薦めてみた
		CFLAG:仲間:好感度 += 100
		CALL COLORPRINT("彼女も気に入ってくれたらしい", カラー_注意)
		PRINTFORMW
	ENDIF
ELSE
	SELECTCASE RAND:3
		CASE 0
			CALL SET_TATOO_RANDOM(対象, STR_FOR_TATOO(-1), 0)
			IF RESULT != -1
				PRINTFORML 彼の指使いはとても巧みで、すぐに心地よい気分に包まれた
				PRINTFORML …%ANAME(対象)%はが気持ちよさにまどろんでいるとタトゥーを勧められた
				PRINTFORML 戸惑う%ANAME(対象)%に対して彼は耳元で甘い誘いの言葉を囁いてくる
				PRINTFORMW 心地よい愛撫のようなその囁きに、%ANAME(対象)%は気づくと頷いていた
				PRINTFORML 
				CALL COLORPRINT(@"%ANAME(対象)%の%GET_TATOO_NAME(RESULT)%に、「%TATOO:対象:RESULT%」とタトゥーを入れられた", カラー_注意)
				PRINTFORMW
				PRINTFORMW %ANAME(対象)%は少々照れながらタトゥーをなぞった
			ELSE
				PRINTFORMW とても心地よかった 
			ENDIF
		CASE 1
			PRINTFORML とても心地よかった 
			PRINTFORMW 優待チケットを貰ったので、また来る事にしよう
		CASE 2
			PRINTFORML マッサージの気持ちよさとアロマの効果で%ANAME(対象)%はトロンとした表情になっている
			PRINTFORML 脱力していると不意にお尻を揉まれ、%ANAME(対象)%はあんっと小さく喘いで身を震わせた
			PRINTFORMW マッサージ師に文句を言おうとしたが続けて秘所を指で優しくなぞられて再び嬌声を上げてしまった
			PRINTFORML いつの間にか体中が敏感になっており、撫でられた所からたまらない快楽が広がり%ANAME(対象)%はゾクゾクと体を震わせる
			PRINTFORML 抵抗しようにもすっかりと蕩けさせられた%ANAME(対象)%は、だらしなく涎を垂らしながら体を跳ねさせることしかできない
			PRINTFORMW 彼は仕込みができたのを確認すると不敵な笑みを浮かべながら%ANAME(対象)%の服を脱がし、その上にのしかかってきた
			PRINTFORML 
			PRINTFORML %ANAME(対象)%はマッサージ台の上で彼の激しいピストンを受けながらヨガリ狂っている
			PRINTFORML 彼によってすっかり出来上がってしまっている%ANAME(対象)%は、ペニスが出入りする度に腰をガクガクと振るわせて喘ぐ
			PRINTFORMW すでに%ANAME(対象)%の理性は圧倒的な快楽の洪水で押し流されており、抵抗も忘れて彼にぎゅうっとしがみついている
			PRINTFORML 膣肉の締め付けに彼もすぐに限界を迎え、%ANAME(対象)%は彼の膣内射精を受けてピンと足を延ばしながら嬌声と共に絶頂した
			PRINTFORML びゅるびゅると胎内にザーメンをたたきつけられ広がる熱に、%ANAME(対象)%は彼にしがみついたままヒィヒィとヨガる
			PRINTFORMW 射精を終えた彼がペニスを引き抜くと、%ANAME(対象)%は秘所からだらしなく白濁液を溢れさせながらアヘ顔を晒して痙攣していた
			PRINTFORML 昂り切った%ANAME(対象)%は次をおねだりするべく、彼に向けてまんこを両手で広げて浅ましい言葉を吐きながら腰をふった
			PRINTFORML その淫らな仕草に彼のペニスは再び勃起し、彼は舌なめずりをしながら%ANAME(対象)%に飛びつきペニスをねじ込んできた
			PRINTFORMW その後も%ANAME(対象)%は身も心もトロトロになるまでたっぷりと膣内マッサージをされた
			CALL FUCK(対象, "欲望, 奉仕, 精愛, 精愛, 性技, 性交, Ｃ, Ｖ, Ｖ, Ｂ, Ｍ, キス, Ｖ性交", "処女喪失, 膣内射精, キス喪失",  GET_SPERM_ID("行きずりの男"), @"マッサージ師の\@RAND:2 ? ペニス # 唇\@", "マッサージ師", "", 喪失_強姦)
			PRINTFORML 
			PRINTFORMW 正気に戻った%ANAME(対象)%は羞恥で頬を染め、彼に見送られながらそそくさと店を後にした
	ENDSELECT
	IF TALENT:対象:美脚 == 0
		PRINTFORML …足がすべすべになった
		TALENT:対象:美脚 = 1
		CALL COLORPRINT(@"%ANAME(対象)%は[美脚]になった", カラー_注意)
		PRINTFORMW
	ELSEIF TALENT:対象:美尻 == 0
		PRINTFORML …お尻が引き締まった
		TALENT:対象:美尻 = 1
		CALL COLORPRINT(@"%ANAME(対象)%は[美尻]になった", カラー_注意)
		PRINTFORMW
	ENDIF
ENDIF
MONEY -= 3000
CALL COLORPRINT("金3000を支払った", カラー_注意)
PRINTFORMW
RETURN 1

