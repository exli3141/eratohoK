	PRINTFORMW 良い気分で%ANAME(対象)%は宮殿へ戻った
	FOR LOCAL, 1, MAX_COUNTRY
		CALL CHANGE_RELATION_C_TO_C(LOCAL, CFLAG:MASTER:所属, 15, -15)
	NEXT
	CALL COLORPRINT("噂が広がり自軍の評判が少しだけ上がった", カラー_注意, "W")
