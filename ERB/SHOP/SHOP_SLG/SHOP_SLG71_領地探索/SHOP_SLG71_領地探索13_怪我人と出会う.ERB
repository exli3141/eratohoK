	金額 = MIN(1000 * (DAY / 5), 10000)
	SIF 金額 == 0
		金額 = 1000
	MONEY += 金額
	CALL COLORPRINT(@"金{金額}を得た", カラー_注意, "W")
	PRINTFORMW 
