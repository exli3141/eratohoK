;-------------------
;●着替え中
;-------------------
@REGISTER_TERRITORY_EVENT125(対象)
#DIM 対象
RETURN 1

@TERRITORY_EVENT125(対象)
#DIM 対象
#DIM 仲間
#DIM 金額
#DIM 候補キャラ, 1000
#DIM 候補数
VARSET 候補キャラ
VARSET 候補数
FOR LOCAL, 0, CHARANUM
	IF CFLAG:(LOCAL):所属 == CFLAG:MASTER:所属 && !CFLAG:(LOCAL):捕虜先 && IS_FEMALE(LOCAL) && !IS_ANIMAL(LOCAL)
		候補キャラ:LOCAL = RAND:1000 
		候補数 ++
	ENDIF
NEXT
IF 候補数 == 0
	仲間 = 0
ELSE
	仲間 = FINDELEMENT(候補キャラ, MAXARRAY(候補キャラ))
ENDIF
PRINTFORML 服を買いに出かけた
PRINTFORMW 気に入った服が見つかったので試着をすることにした
IF IS_MALE(対象)
	IF 仲間 == 0 || 対象 != MASTER
		PRINTFORML …なかなかカッコいい服だ
		PRINTFORMW 良い服が買えたとご機嫌で帰路についた
		金額 = 1000 + 10 * DAY
		MONEY -= 金額
		CALL COLORPRINT(@"金{金額}を支払った", カラー_注意, "W")
	ELSE
		PRINTFORML 試着室に入ると、%ANAME(仲間)%の着替えに鉢合わせしてしまった！
		PRINTFORML 着替え中だった%ANAME(仲間)%は肌も露わな格好だ
		CALL ASK_MULTI("カーテンを閉める" ,"試着室の中へ押し入る")
		IF RESULT == 0
			PRINTFORML 謝りながら慌ててカーテンを閉めたが、彼女のなまめかしい肌が目に焼き付いた
			PRINTFORML %ANAME(仲間)%が試着室から出てきてジトッとした目つきで睨むので再び謝った
			PRINTFORML 彼女は指を立ててお詫びとしてショッピングに付き合うように要求してきた
			PRINTFORMW それで許されるならと、荷物持ちとして同行することにした
			金額 = 2000 + 40 * DAY
			SIF 金額 > MONEY
				金額 = MONEY
			MONEY -= 金額
			CFLAG:仲間:好感度 += 200
			CFLAG:仲間:面識 = 1
			CALL COLORPRINT(@"金{金額}を支払った", カラー_注意)
			PRINTFORML
			CALL COLORPRINT(@"%ANAME(仲間)%の評価が上がった", カラー_注意)
			PRINTFORMW
		ELSE
			PRINTFORML 彼女のなまめかしい肌に欲情してしまった%ANAME(対象)%は、試着室の中に押し入りカーテンを閉めた
			PRINTFORML 下着姿で慌てる%ANAME(仲間)%に掴み掛ると、下着を剥ぎ全裸の彼女を犯し始めた
			PRINTFORML カーテン一枚向こうに他の客や店員のいる状況で%ANAME(仲間)%を犯す異常な状況に興奮した%ANAME(対象)%は、そのまま何度も彼女に放った
			PRINTFORML 
			PRINTFORMW %ANAME(対象)%が立ちながら犯していた%ANAME(仲間)%の中からペニスを引き抜くと、彼女は全裸で床にくたりと跪いた
			PRINTFORML 跪いた彼女の顔にドビュッ！と射精して白濁塗れにすると、膣内から溢れた精液で床を汚している%ANAME(仲間)%を置いて%ANAME(対象)%は試着室から出て行った
			PRINTFORML 精液塗れのドロドロの姿で、あの後%ANAME(仲間)%はどうやって帰ったのだろう？
			CALL FUCK(仲間, "Ｃ, Ｂ, Ｖ, Ｍ, 欲望, 性交, 奉仕, 精愛, 口淫, キス, Ｖ性交", "処女喪失, キス喪失, 膣内射精", GET_ID(対象), @"%ANAME(対象)%の\@RAND:2 ? ペニス # 唇\@", ANAME(対象), "", 喪失_強姦)
			CALL FUCK(対象, "性技, 性交, Ｃ, 射精", "童貞喪失, キス喪失", 0, @"%ANAME(仲間)%の唇", "", @"%ANAME(仲間)%の膣", 喪失_強姦)
			CFLAG:仲間:好感度 -= 400
			CFLAG:仲間:面識 = 1
			CALL COLORPRINT(@"%ANAME(仲間)%の評価が下がった", カラー_注意)
			PRINTFORMW
		ENDIF
	ENDIF
ELSE
	IF RAND:2 == 0
		PRINTFORML …なかなか素敵な服だ
		PRINTFORMW 良い服が買えたとご機嫌で帰路についた
		金額 = 1000 + 10 * DAY
		MONEY -= 金額
		CALL COLORPRINT(@"金{金額}を支払った", カラー_注意, "W")
	ELSE
		PRINTFORML 試着室で着替えていると突然目の前の鏡が開き、そこから伸びてきた腕につかまれた
		PRINTFORML いきなりの事に叫び声を出そうとしたが、口をふさがれてしまいそのまま引きずり込まれた
		PRINTFORMW …誰もいなくなった試着室に、%ANAME(対象)%の上着だけが残された
		PRINTFORML 
		PRINTFORML 薄暗い地下室で%ANAME(対象)%が犯されている
		PRINTFORML %ANAME(対象)%は目隠しをされ腕を縛られた状態でならず者のペニスをねじ込まれ、喘ぎ声を上げて身悶える
		PRINTFORML 拉致されてから延々と男に好き放題に犯され続けながらも、%ANAME(対象)%は涙を流しつつ何とか堪えようとしている
		PRINTFORML しかしその意に反して体はすっかり蕩けてしまっており、収縮を繰り返してきゅうきゅうとペニスを締め付けている
		PRINTFORML その反応に男はますます興奮して荒々しく腰を打ち付けてきて、%ANAME(対象)%は望まぬ快楽で思わず身をくねらせてしまう
		PRINTFORML 男が気持ちよさそうに呻いてザーメンを放つと、%ANAME(対象)%はひぃひぃと喘ぎながら抗えない絶頂を味わわされた
		PRINTFORMW その後も延々と犯され続け、両の指で足りない程に絶頂させられる頃には%ANAME(対象)%は目隠しの下でだらしないアヘ顔をしていた
		CALL FUCK(対象, "欲望, 奉仕, 精愛, 性技, 性交, Ｃ, Ｖ, Ｂ, Ａ, Ｍ, キス, 口淫, 緊縛, マゾ, Ｖ性交, Ａ性交", "処女喪失, Ａ処女喪失, 膣内射精, キス喪失, 腸内射精, 口内射精", GET_SPERM_ID("痴漢"), @"暴漢の\@RAND:2 ? ペニス # 唇\@", "暴漢", "", 喪失_強姦)
		PRINTFORML 
		PRINTFORMW %ANAME(対象)%は男に飽きられるまで犯された後、適当な路地裏に捨てられた
	ENDIF
ENDIF
RETURN 1

