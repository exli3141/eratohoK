;-------------------------------------------------
;選択可否判定
;-------------------------------------------------
@COM_ABLE2
;共通部分
CALL COM_ABLE_COMMON(2)
SIF RESULT == 0
	RETURN 0
;プレイヤーは最大で1人まで
SIF MPLY_NUM <= 0 || MPLY_NUM > 1
	RETURN 0
;ターゲットは最大で1人まで
SIF MTAR_NUM <= 0 || MTAR_NUM > 1
	RETURN 0
;対象にＶが必要
SIF !HAS_VAGINA(MTAR:0)
	RETURN 0
;プレイヤー行動不能なら不可
SIF !IS_PLAYABLE(MPLY:0)
	RETURN 0
;挿入中は不可
SIF IS_INSERT_MUTUAL(MPLY:0, MTAR:0)
	RETURN 0
;性器に愛液、母乳以外の汚れがあり、実行者に汚れ無視がない場合不可
;SIF (STAIN:(MTAR:0):3 & 14) && !TALENT:(MPLY:0):汚れ無視
;	RETURN 0
;プレイヤーとターゲットが共に拘束中なら不可
SIF IS_BIND(MPLY:0) && IS_BIND(MTAR:0)
	RETURN 0
;プレイヤーが顔面騎乗されている場合
IF IS_RIDDEN(MPLY:0)
	;ターゲット以外のキャラに顔面騎乗されているなら不可
	IF !IS_RIDDEN(MPLY:0, MTAR:0)
		RETURN 0
	ENDIF
	;Ａ顔面騎乗なら不可
	IF IS_EQUIP_PLAYER(MTAR:0, 102)
		RETURN 0
	ENDIF
ELSE
	;ターゲットがプレイヤー以外に顔面騎乗しているなら不可
	IF IS_RIDE(MTAR:0)
		RETURN 0
	ENDIF
ENDIF
;土下座しているなら不可
SIF IS_EQUIP_PLAYER(MPLY:0, 110)
	RETURN 0
;プレイヤーが胸愛撫・キス中なら不可
SIF IS_EQUIP(MPLY:0, 1, 20)
	RETURN 0
;ターゲットがクリキャップ装着中なら不可
SIF IS_EQUIP_TARGET(MTAR:0, 63)
	RETURN 0
;プレイヤーの口がふさがってたらだめ
SIF IS_M_HOLD(MPLY:0)
	RETURN 0
;ターゲットからプレイヤーに足舐め中なら不可
SIF SEARCH_EQUIP_IC(104, MPLY:0, MTAR:0) >= 0
	RETURN 0
;まんこに突っ込まれてるならダメ
SIF IS_INSERT_SINGLE(MTAR:0, "Ｖ")
	RETURN 0
RETURN 1

