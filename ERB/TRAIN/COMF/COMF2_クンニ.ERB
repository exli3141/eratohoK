ELSEIF LOCAL:0 || SEARCH_EQUIP(11, MTAR:0, MPLY:0) >= 0 || SEARCH_EQUIP(2, MTAR:0, MPLY:0) >= 0
	IF HAS_PENIS(MTAR:0) && HAS_VAGINA(MTAR:0)
		LOCALS:0 = シックスナイン(Ｃ)
	ELSE
		LOCALS:0 = シックスナイン
	ENDIF
